{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-teradata-test"
		},
		"lsdls1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'lsdls1'"
		},
		"lssql1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lssql1'"
		},
		"lssql2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lssql2'"
		},
		"lsdls1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlsland001.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-teradata-test_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"lsdls1_accountKey": {
						"value": "[parameters('lsdls1_accountKey')]"
					},
					"lssql1_connectionString": {
						"value": "[parameters('lssql1_connectionString')]"
					},
					"lssql2_connectionString": {
						"value": "[parameters('lssql2_connectionString')]"
					},
					"lsdls1_properties_typeProperties_url": {
						"value": "[parameters('lsdls1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}